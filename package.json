{
  "name": "css-to-react-native",
  "version": "2.1.1",
  "description": "Convert CSS text to a React Native stylesheet object",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --ignore test.js --out-dir dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/styled-components/css-to-react-native.git"
  },
  "keywords": [
    "styled-components",
    "React",
    "ReactNative",
    "styles",
    "CSS"
  ],
  "author": "Jacob Parker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/styled-components/css-to-react-native/issues"
  },
  "homepage": "https://github.com/styled-components/css-to-react-native#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^22.2.2",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "css-color-keywords": "^1.0.0",
    "fbjs": "^0.8.5",
    "postcss-value-parser": "^3.3.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
